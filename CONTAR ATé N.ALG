algoritmo "Contar até N"
var
   M, L : inteiro
inicio
   EscrevaL("Contando ate pedir para parar")
   Escreva("Digite o numero Inicial: ")
   Leia(M)
   Escreva("Ate quando deseja contar? ")
   Leia(L)
   escrevaL(M)
   enquanto (M <> L)faca
      M <- M + 1
      escrevaL(M)
   FimEnquanto
   Escreva("Contagem Finalizada")
fimalgoritmo